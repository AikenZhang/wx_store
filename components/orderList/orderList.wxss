@import "../../app.wxss";
.fyOrder-list{
  padding: 15rpx;
}
.fyOrder-list-header{
  width: 100%;
  height: 80rpx;
  background: #f0f0f0;
}
.fyOrder-list-tag{
  font-size:16px;
  color:#ed7098;
  height:80rpx;
  line-height: 80rpx;
  padding: 0 25rpx;
}
.fyOrder-list-content{
  height: 370rpx;
  background: #fff;
}
.fyOrder-list-info{
  height: 250rpx;
  border-bottom:1rpx solid #f2f2f2;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.fyOrder-list-info-text{
  width:350rpx;
  height: 200rpx;
  font-size:15px;
  padding: 0 10rpx;
}
.fyOrder-Model{
  font-size:14px;
  color:#666;
}
.fyOrder-list-bbar{
  height: 120rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.fyOrder-list-bbar view{
  margin:0 10rpx;
}
.fyOrder-list-info-price{
  flex: 1;
  width:150rpx;
  height: 200rpx;
  color:#666;
  font-size:14px;
}
.fyOrder-list-info-img{
  width:200rpx;
  height:200rpx;
  margin-left:20rpx;
}