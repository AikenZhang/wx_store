.fy-address{
  width:100%;
  height:100%;
}
.fy-address-item{
  width:100%;
  border-bottom: 1rpx solid #f2f2f2;
  margin:0 auto;
  background: #fff;
}
.fy-address-switch{
  margin-top:100rpx;
  border-top:1rpx solid #f2f2f2;
  border-bottom:1rpx solid #f2f2f2;
  background: #fff;
  font-size:14px;
  display: flex;
  justify-content: space-between;
  height:100rpx;
  align-items: center;
}
.fy-address-seitchtitle{
  padding: 0 10rpx;
}
.fy-address-but{
  display: flex;
  justify-content: space-between;
  margin-top:100rpx;
}
.fy-address-but button {
  font-size:15px;
  height:65rpx;
  line-height: 65rpx;
  border:0;
  color:#fff;
}