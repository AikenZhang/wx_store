/* pages/confirmOrder/index.wxss */@import "../../app.wxss";
.fy-cell{
  display: flex;
  height: 120rpx;
  background: #fff;
  margin:10rpx 0;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.fy-cell-icon{
  padding: 0 15rpx;
  color:#333;
  font-size:13px;
}
.fy-cell-navto{
  color:#333;
  font-size:13px;
  flex:1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.fy-cell-arrow{
  padding: 0 15rpx;
  font-size:13px;
}
.fy-cell-arrow span{
  font-size:12px;
}
.fy-cell-title {
  font-size:14px;
  color:#000;
  font-weight: 600;
}
.fy-cell-content{
  width:600rpx;
  font-size:11px;
  color:#888;
}
.fy-order {
  width: 100%;
  padding: 0 15rpx;
  background: #fff;
  box-sizing: border-box;
}
.fy-order-sum {
  display: flex;
  justify-content: space-between;
  border-bottom:1rpx solid #f2f2f2;
  font-size:19px;
  color:#000;
  font-weight: 400;
  height:100rpx;
  line-height:100rpx;

}
.fy-order-content {
  font-size:13px;
  border-bottom: 1px solid #f2f2f2;
}
.fy-order-item{
  display: flex;
  height:70rpx;
  line-height:70rpx;
  justify-content: space-between;
  color:#888;
  margin-bottom:1rpx;
}
.fy-order-item-title{
  width:300rpx;
}
.fy-order-but{
  width:100%;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
  font-size:17px;
  }
.fy-order-but{
  width:100%;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
  font-size:17px;
  color:#f981a7;
}
.fy-popup-info{
  width:100%;
  background: #f6f6f6;
}
